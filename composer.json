{
    "name": "jakubboucek/esc-pos",
    "description": "Native ESC-POS drivers",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jakub Bouček",
            "email": "pan@jakubboucek.cz"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-iconv": "*",
        "ext-sockets": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "JakubBoucek\\EscPos\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "phpstan": "phpstan analyze src -c phpstan.neon --level 6"
    }
}
